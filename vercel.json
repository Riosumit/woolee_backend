{
    "version": 2,
    "builds": [
      {
        "src": "woolee_backend/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.9"
        }
      },
      {
        "src": "interface/build/**",
        "use": "@vercel/static"
      },
      {
        "src": "staticfiles/**",
        "use": "@vercel/static"
      },
      {
        "src": "media/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
        { 
            "src": "/static/(.*)",
             "dest": "staticfiles/$1" 
        },

        { 
            "src": "/media/(.*)", 
            "dest": "media/$1" 
        },
        {
            "src": "/(.*)",
            "dest": "woolee_backend/wsgi.py"
        }
    ]
  }